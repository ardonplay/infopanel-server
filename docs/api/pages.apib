                "order_id": 1,
                "content": [
                        {
                            "id": "7ea7e09a-5bc1-4925-9182-a57f6c91339d"
                        },
                         {
                            "id": "6b049dad-7966-4047-b712-3ddff3fd1401"
                        }
                    ]
            }

    + Schema

            {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                     "type": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
